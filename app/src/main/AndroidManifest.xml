<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jaenx.muestraapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Colecciones.ColeccionActivity"
            android:label="@string/title_activity_coleccion"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ColeccionDetail.ColeccionDetailActivity"
            android:label="@string/title_activity_coleccion_detail"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".addeditcoleccion.AddEditColeccionActivity"
            android:label="@string/title_activity_add_edit_coleccion"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>